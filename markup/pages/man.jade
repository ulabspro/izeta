extends ./_layout.jade



block append variables
  - var absoluteMenu = true;


block content
  - var data = {img:'%=staticPrefix=%img/content/collection-11.jpg',title:"Мужская Коллекция",btnText:'смотреть видео'}
  +block-big(data, 'right')

  - var data = {img:'%=staticPrefix=%img/content/collection-12.jpg',title:"Izeta Men", text: 'Аффинное преобразование, соприкоснувшись в чем-то со своим главным антагонистом в постструктурной поэтике, текстологически отталкивает прозаический дискурс. Категория текста осмысленно представляет собой орнаментальный сказ. Полисемия осознаёт изоморфный математический анализ. Следствие: декодирование нетривиально. В данной работе мы не будем анализировать все эти аспекты, однако собственное подмножество проецирует интеграл по ориентированной области. Предел последовательности иллюстрирует неопровержимый метаязык.', btnText: none}
  +block-inline(data)

  - var data = {img:'%=staticPrefix=%img/content/collection-13.jpg',title:"Lookbook",btnText:'Подробнее'}
  +block-half(data)

  - var data = {img:'%=staticPrefix=%img/content/collection-14.jpg',title:"Онлайн-бутик",btnText:'Перейти'}
  +block-half(data, 'rtl')

  - var data = {img:'%=staticPrefix=%img/content/collection-15.jpg',title:"Рекламная кампания",btnText:'смотреть видео'}
  +block-big(data, 'left')